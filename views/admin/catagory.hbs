          </div>
          </div>
          </div>
          
          </main>
      <div class="one">
        <table class="table table-striped table-sm" method="post" action="">
          <thead>
            <tr>
              <th scope="col" >catagory</th>
              <th scope="col" >image</th>
              <th scope="col" >discription</th>
              <th scope="col" >delete</th>
              <th scope="col">edit</th>
            </tr>
          {{#each result}}
           <tr>
           <td>{{this.catagory}}</td>
           <td><img src="/images/{{this.image}}" height="100" width="100px"></td>
           <td>{{this.discription}}</td>
           <td><button><a href ="catagory/{{this._id}}">delete</button></a></td>
           <td><button><a href="edit/{{this._id}}">edit</a></button></td>
           </tr>
           {{/each}}

          </thead>
          <tbody>
            
          </tbody>
        </table>
      </div>
    </main>
  </div>
</div>


